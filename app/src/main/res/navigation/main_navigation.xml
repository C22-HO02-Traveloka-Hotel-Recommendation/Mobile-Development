<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_navigation"
    app:startDestination="@id/loginFragment">


   <fragment
        android:id="@+id/loginFragment"
        android:name="com.company.project.traveloka.ui.login.LoginFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_login" >
       <action
           android:id="@+id/action_loginFragment_to_registerFragment"
           app:destination="@id/registerFragment" />
   </fragment>
    <fragment
        android:id="@+id/registerFragment"
        android:name="com.company.project.traveloka.ui.register.RegisterFragment"
        android:label="fragment_register"
        tools:layout="@layout/fragment_register" />
    <activity
        android:id="@+id/profileActivity"
        android:name="com.company.project.traveloka.ui.profile.ProfileActivity"
        android:label="activity_profile"
        tools:layout="@layout/activity_profile" />

</navigation>